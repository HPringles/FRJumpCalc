{
  "name": "frjump",
  "version": "0.1.1",
  "productName": "frjump",
  "description": "fuel rats case jump calculator",
  "main": "main.js",
  "scripts": {
    "package-win": "electron-packager . --asar --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --icon=public/media/fuelrats2_slb_icon.ico --version-string.CompanyName=FR --version-string.FileDescription=FR --version-string.ProductName=\"frjump\"",
    "create-installer-win": "node installers/createinstaller.js"
  },
  "keywords": [
    "fuelrats",
    "jump",
    "calculator"
  ],
  "directories": {
    "buildResources": [
      "public",
      "views"
    ]
  },
  "files": [
    "*.js",
    "views",
    "public",
    "node_modules"
  ],
  "author": "HPringles",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "electron": "^1.7.10",
    "electron-winstaller": "^2.6.3"
  },
  "dependencies": {
    "autocorrect": "^1.2.0",
    "ed-logwatcher": "^1.4.0",
    "electron-config": "^1.0.0",
    "electron-debug": "^1.4.0",
    "electron-oauth2": "^3.0.0",
    "request": "^2.83.0",
    "windows-shortcuts": "^0.1.6",
    "ws": "^3.3.3"
  }
}
